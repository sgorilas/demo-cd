pipeline {
     agent any
     stages {


         stage('compile') {
                     steps {
                     sh 'chmod +x mvnw'
                     sh './mvnw clean compile'
                     }
                 }
         stage('unit test') {
                     steps {
                         sh './mvnw clean test'
                     }
                 }


                 stage('build') {
                             steps {
                                 sh './mvnw -DskipTests package'
                             }
                         }
     }

 }