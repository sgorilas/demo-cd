pipeline {
     agent any
     stages {


         stage('compile') {
                     steps {
                         sh 'mvnw -DskipTests clean compile'
                     }
                 }
         stage('unit test') {
                     steps {
                         sh 'mvnw clean test'
                     }
                 }


                 stage('build') {
                             steps {
                                 sh 'mvnw -DskipTests clean package'
                             }
                         }
     }

 }